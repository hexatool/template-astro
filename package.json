{
	"name": "@hexatool/template-astro-preact",
	"version": "0.0.0",
	"author": "Hexatool",
	"description": "A template for Astro projects with Preact",
	"keywords": [
		"astro",
		"eslint",
		"prettier",
		"tailwindcss",
		"typescript",
		"preact"
	],
	"homepage": "https://github.com/hexatool/template-astro-preact",
	"bugs": {
		"url": "https://github.com/hexatool/template-astro-preact"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:hexatool/template-astro-preact.git"
	},
	"license": "PRIVATE",
	"private": true,
	"files": [
		"dist"
	],
	"type": "module",
	"scripts": {
		"build": "astro build",
		"build:types": "astro sync",
		"dev": "astro dev",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"preview": "astro preview",
		"typecheck": "astro check && tsc --noEmit"
	},
	"dependencies": {
		"@fontsource/inter": "5.0.16",
		"@nanostores/i18n": "0.12.0",
		"@nanostores/persistent": "0.9.1",
		"@nanostores/preact": "0.5.0",
		"astro": "4.0.8",
		"astro-seo": "0.8.0",
		"lucide-astro": "0.363.0",
		"nanostores": "0.9.5",
		"preact": "10.19.4"
	},
	"devDependencies": {
		"@astrojs/check": "0.5.6",
		"@astrojs/preact": "3.1.1",
		"@astrojs/prefetch": "0.4.1",
		"@astrojs/sitemap": "3.1.1",
		"@astrojs/tailwind": "5.1.0",
		"@astrojs/ts-plugin": "1.5.3",
		"@hexatool/eslint-config": "2.3.0",
		"@hexatool/tsconfig": "20.0.1",
		"@types/bun": "1.0.7",
		"@vite-pwa/astro": "0.3.0",
		"astro-compress": "2.2.10",
		"astro-eslint-parser": "0.16.3",
		"astro-robots-txt": "1.0.0",
		"astro-webmanifest": "1.0.0",
		"eslint": "8.57.0",
		"eslint-plugin-astro": "0.31.4",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"tailwindcss": "3.4.1",
		"typescript": "5.3.3"
	},
	"overrides": {
		"react": "npm:@preact/compat@latest",
		"react-dom": "npm:@preact/compat@latest"
	},
	"trustedDependencies": [
		"sharp"
	],
	"engines": {
		"node": "20"
	}
}
